{
"manifest_version": 2,
    "name": "[your extension name here]",
    "description": "[your extension description here]",
    "version": "1.0",
    "background" : {
        "scripts" : ["background.js"]
    },
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ],
    "browser_action": {
      "default_icon": "logo.png",
      "default_popup": "popup.html"
    },
    "permissions": ["activeTab"]
}
